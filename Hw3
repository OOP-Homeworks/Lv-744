        static void Task5() 
        {
            int year = int.Parse(Console.ReadLine());

            if (year == 366)
            {
                Console.WriteLine("Leap year");
            }
            else
            {
                Console.WriteLine("Not Leap");
            }
        }
        static void Task6() 
        {
            int a = int.Parse(Console.ReadLine());

            int first = a % 10;
            int second = a / 100;
            int third = (a / 10) % 10;
            int sum = first + second + third;
            Console.WriteLine(sum);    
        }
       static bool Task7(int n)
        {
            while (n > 0)
            {
                int first = n % 10;
                if (first % 2 == 0)
                {
                    return false;
                }
                n /= 10;
            }
            return true;
        }
        static int Hw1(string str) 
        {
            
            char[] array = str.ToCharArray();
            int sum = 0;

            for (int i = 0; i < array.Length; i++)
            {
                if (array[i] == 'a' || array[i] == 'o' || array[i] == 'i' || array[i] == 'e') 
                {
                    sum++;
                }
            }

            return sum;
        }
        static int Hw2(int month) 
        {
            
            
            switch (month) 
            {
                case 1:
                    return 31;
                    break;
                case 2:                    
                    return 28;
                    break;
                case 3:                   
                    return 31;
                    break;
                case 4:
                    return 30;
                    break;                   
                case 5:
                    return 31;
                    break;
                case 6:
                    return 30;
                    break;
                case 7:
                    return 31;
                    break;
                case 8:
                    return 31;
                    break;
                case 9:
                    return 30;
                    break;
                case 10:
                    return 31;
                    break;
                case 11:
                    return 30;
                    break;
                case 12:
                    return 30;
                    break;
                default:
                    return 0;
            }
            
        }
        static void Hw3() 
        {
            try 
            {
                int[] array = new int[9];
                int sum = 0;
                int product = 1;

                Console.WriteLine("Enter a array of 10 elements: ");
                for (int i = 0; i < array.Length; i++)
                {
                    array[i] = int.Parse(Console.ReadLine());
                }
                for (int i = 0; i < array.Length; i++)
                {
                    if (array[i] > 0 && i < 5)
                    {
                        sum += array[i];
                    }
                    if (i > 5)
                    {
                        product *= array[i];
                    }
                }

                if (product == 0)
                    Console.WriteLine(sum);
                else
                    Console.WriteLine(product);
            }
            catch (Exception e)
            {
                Console.WriteLine(e.Message);
            }
            
            
        }

    }
