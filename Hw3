        static void Task1() 
        {
            int a = int.Parse(Console.ReadLine());
            int b = int.Parse(Console.ReadLine());

            int count = 0;

            for (int i = 0; a <= b; i++)
            {
                if (a % 3 == 0 || a + 1 % 3 == 0)
                {
                    count++;


                }
                a++;

            }
            Console.WriteLine(count);


            Console.WriteLine();
        }
        static void Task2(string test) 
        {
            
            char[] chars= test.ToCharArray();

            for (int i = 0; i < test.Length; i++)
            {
               if(i % 2 !=0) 
                {
                    Console.WriteLine(chars[i]);
                }
            }

        }
        static void Task3() 
        {
            string[] str = { "Coffee", "Tea", "juice", "water" };

            for (int i = 0; i < str.Length; i++)
            {
                if (str[i] == "Coffee")
                {
                    Console.WriteLine("Coffee cost 3 eur");
                }
                if (str[i] == "Tea")
                {
                    Console.WriteLine("Tea cost 3 eur");
                }
                if (str[i] == "juice")
                {
                    Console.WriteLine("Juice cost 3 eur");
                }
                if (str[i] == "water")
                {
                    Console.WriteLine("Water cost 3 eur");
                }

            }
            
        }
        static void Task4() 
